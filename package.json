  "dependencies": {
    "jquery": "^3.5.0",
    "bootstrap": "^4.3.1",
    "reactstrap": "^6.0.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-router-dom": "^4.2.2",
    "moment": "^2.22.2",
    "react-moment": "^0.7.0",
    "axios": "^0.18.0",
